# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  basis_config = debug

else ifeq ($(config),release)
  basis_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := basis

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

basis:
ifneq (,$(basis_config))
	@echo "==== Building basis ($(basis_config)) ===="
	@${MAKE} --no-print-directory -C . -f basis.make config=$(basis_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f basis.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   basis"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"